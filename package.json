{
  "name": "cli-deprecate",
  "version": "1.0.0",
  "description": "Deprecation warning utility",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-deprecate"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-deprecate/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/freeformsystems/cli-deprecate/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "deprecate",
    "warn"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.3.2",
    "mocha": "^1.21.4"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "readme": "mdp --force -v",
    "test": "NODE_ENV=test istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/spec"
  },
  "mdp": {
    "title": "Deprecate",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/cli-deprecate",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "test.md"
        ]
      },
      {
        "inc": [
          "license.md"
        ]
      }
    ]
  }
}
